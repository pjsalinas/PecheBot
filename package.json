{
  "name": "pechebot",
  "version": "1.0.0",
  "description": "A bot which can answer questions for me, and can handle request from different platforms.",
  "main": "bot.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "claudia create --region us-east-1 --api-module bot",
    "update": "claudia update",
    "configure-slack-slash-command": "claudia update --configure-slack-slash-command",
    "configure-twilio": "claudia update --configure-twilio-sms-bot",
    "configure-telegram": "claudia update --configure-telegram-bot",
    "configure-skype": "claudia update --configure-skype-bot"
  },
  "keywords": [
    "bot",
    "pechebot",
    "slack",
    "skype",
    "telegram",
    "twilio"
  ],
  "author": "Pedro J. Salinas <pedrojsalinas@gmail.com> (http://www.pechebot.com)",
  "license": "MIT",
  "dependencies": {
    "claudia-bot-builder": "^2.15.0",
    "lodash": "^4.17.4",
    "twilio": "^3.0.0"
  },
  "devDependencies": {}
}
